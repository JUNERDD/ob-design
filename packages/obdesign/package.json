{
  "name": "obdesign",
  "type": "module",
  "version": "0.1.1",
  "description": "一个基于 Vue 3 和 Unocss 的组件库，包含 Unocss 预设",
  "author": "JUNER",
  "license": "MIT",
  "homepage": "https://github.com/JUNERDD/ob-design",
  "keywords": [
    "vue",
    "components",
    "unocss",
    "preset",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./types.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "require": "./dist/preset.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.cjs"
    },
    "./auto": {
      "types": "./dist/auto.d.ts",
      "import": "./dist/auto.mjs",
      "require": "./dist/auto.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./types.d.ts",
  "scripts": {
    "patch": "pnpm version patch",
    "build": "pnpm unbuild"
  },
  "dependencies": {
    "@obdesign/components": "latest",
    "@obdesign/preset": "latest"
  },
  "devDependencies": {
    "unbuild": "^2.0.0"
  }
}
